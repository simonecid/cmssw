Universe = vanilla
job = l1tGenMuonMatching_QCD_15_3000_NoPU_Phase1
cmd = L1TJetConvolutionCurves/MatchL1TMuonWithGenLevelMuons/test/runCMSSWWrapper.sh
args= -j $(job) -c $(cluster) -p $(process) -i L1TJetConvolutionCurves/MatchL1TMuonWithGenLevelMuons/python/ConfFile_cfg.py -d l1tMuonGenMuonMatching_QCD_15_3000_NoPU_Phase1_WQualityBranch
